{
  "name": "ekp-cryptozoon",
  "version": "0.1.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start --watch",
    "start:prod": "node dist/main",
    "gen-secret-key": "werf helm secret generate-secret-key > .werf_secret_key",
    "encrypt-secrets": "werf helm secret values encrypt clear-values.yaml -o .helm/secret-values.yaml",
    "gh-set-secret-key": "gh secret set WERF_SECRET_KEY --repos=\"cryptozoon\" < .werf_secret_key",
    "gh-set-do-creds": "gh secret set KUBE_CONFIG_BASE64_DATA --repos=\"cryptozoon\" -b$(doctl kubernetes cluster kubeconfig show ekp | base64)"
  },
